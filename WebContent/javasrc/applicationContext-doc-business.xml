	<bean id="objectTypeServiceForCirculation" class="jp.co.ctc_g.eim.framework2.business.service.impl.ObjectTypeServiceImpl" >
		<property name="objectTypeDao">
			<ref bean="objectTypeDaoForCirculation"/>
		</property>
	</bean>

	<!-- 回付状況確認用ステータスサービス -->
	<bean id="statusServiceForCirculation" class="jp.co.ctc_g.eim.framework2.business.service.impl.StatusServiceImpl" >
