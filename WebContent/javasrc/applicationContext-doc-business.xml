	<bean id="documentObjectTypeLayoutService" class="jp.co.ctc_g.eim.app.document.business.service.impl.DocumentObjectTypeLayoutServiceImpl">
		<property name="objectTypeService" ref="objectTypeServiceForDocFormType"/>
		<property name="objectService" ref="formLayoutObjectService2"/>
		<property name="attributeTypeLayoutService" ref="attributeTypeLayoutService"/>
		<property name="attributeTypeService" ref="attributeTypeService2"/>
		<property name="nameSpaceListForNewCopyFlagUpdate">
			<list>
				<value>${NAMESPACE_FORM}</value>
				<value>${APP_TASK_NAMESPACE}</value>
			</list>
		</property>
	</bean>

	<!-- ドキュメントレイアウト情報保持オブジェクトタイプのサービス(値リストを取得する)  -->
	<bean id="documentObjectTypeLayoutServiceForHandlingValueList"  parent="documentObjectTypeLayoutService">
		<property name="handlingVlueList" value="true"/>
	</bean>

	<!-- WebDAVサービス -->
	<bean id="documentWebDAVService" class="jp.co.ctc_g.eim.app.document.business.service.impl.DocumentWebDAVServiceImpl" >
		<property name="objectService">
			<ref bean="objectService2"/>
		</property>
		<property name="formatService">
			<ref bean="formatService2"/>
		</property>
		<property name="fileAccessService">
			<ref bean="docfileAccessService"/>
		</property>
