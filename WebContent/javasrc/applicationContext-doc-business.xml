		<property name="fileIoService" ref="fileIoService"/>
		<property name="qrCodeService" ref="qrCodeService"/>
		<property name="pdfService" ref="pdfService"/>
	</bean>

	<!-- 添付ファイルオブジェクトサービス -->
	<bean id="dcoumentFormTempFileObjectService" parent="formTempFileObjectService2" >
		<property name="objectService">
			<ref bean="dcoumentFormTempFileObjectObjectService"/>
		</property>
	</bean>

	<!-- オブジェクトサービス（添付ファイルオブジェクト用） -->
	<bean id="dcoumentFormTempFileObjectObjectService" parent="objectService2" >
		<property name="objectTypeDao">
			<ref bean="dcoumentFormTempFileObjectObjectTypeDao"/>
		</property>
	</bean>

