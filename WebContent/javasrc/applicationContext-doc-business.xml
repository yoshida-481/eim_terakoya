		<property name="fileAccessService" ref="fileAccessService2"/>
		<property name="formatService" ref="formatService2"/>
		<property name="accessHistoryService" ref="accessHistoryService2"/>
		<property name="formAttachedFileService" ref="formAttachedFileService"/>
	</bean>

