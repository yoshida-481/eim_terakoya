	<bean id="folderUploadService" class="jp.co.ctc_g.eim.app.document.business.service.impl.FolderUploadServiceImpl">
		<property name="workspaceService" ref="workspaceService2"/>
		<property name="folderService" ref="folderService2"/>
		<property name="documentService" ref="documentService2"/>
		<property name="attributeTypeService" 	ref="attributeTypeService2"/>
		<property name="operationHistoryService" 	ref="operationHistoryService2"/>
