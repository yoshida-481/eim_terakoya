<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="
			http://www.springframework.org/schema/beans
			http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<!-- objectServiceLayout -->
	<bean id="objectServiceLayout" class="jp.co.ctc_g.eim.framework2.business.service.impl.ObjectServiceImpl">
		<property name="objectDao">
			<ref bean="objectDaoLayout"/>
		</property>
		<property name="objectTypeDao">
			<ref bean="objectTypeDao2"/>
		</property>
		<property name="fileDao">
			<ref bean="fileDao2"/>
		</property>
		<property name="fileAccessor">
			<ref bean="fileAccessor2"/>
		</property>
		<property name="relationDao">
			<ref bean="relationDao2"/>
		</property>
		<property name="userDao">
			<ref bean="userDaoLight2"/>
		</property>
		<property name="accessRoleTypeDao">
			<ref bean="accessRoleTypeDao2"/>
		</property>
	</bean>
	
	<!-- objectTypeServiceLayout -->
	<bean id="objectTypeServiceLayout" class="jp.co.ctc_g.eim.framework2.business.service.impl.ObjectTypeServiceImpl" >
		<property name="objectTypeDao">
			<ref bean="objectTypeDaoLayout"/>
		</property>
	</bean>
	
	<!-- securityServiceLight2 -->
	<bean id="securityServiceLight2" class="jp.co.ctc_g.eim.framework2.business.service.impl.SecurityServiceImpl" >
		<property name="securityDao">
			<ref bean="securityDaoLight2"/>
		</property>
	</bean>
	
	<!-- objectService2 -->
	<bean id="formObjectService2ForAttributeAll" class="jp.co.ctc_g.eim.framework2.business.service.impl.ObjectServiceImpl">
		<property name="objectDao" ref="formObjectDao2ForAttributeAll" />
		<property name="fileDao" ref="fileDao2" />
		<property name="fileAccessor" ref="fileAccessor2" />
		<property name="relationDao" ref="relationDao2" />
		<property name="userDao" ref="userDaoLight2" />
		<property name="accessRoleTypeDao" ref="accessRoleTypeDao2" />
		<property name="objectTypeDao" ref="formObjectTypeDao2" />
		<property name="namingService" ref="namingService2" />
	</bean>
</beans>