		<UserDefGroup key="workSpaceAdminInRecycleUserDefGroupPlugIn" id="-12001">
			<supportGetSearchCondition>true</supportGetSearchCondition>
			<supportGetUserList>true</supportGetUserList>
			<nameList>
				<name lang="JA" langname="ワークスペース管理者（ごみ箱配下表示用）" />
				<name lang="EN" langname="WorkSpaceAdmin User" />
			</nameList>
		</UserDefGroup>

		<!-- ===================== -->
		<!-- 帳票管理              -->
		<!-- ===================== -->

		<!-- ユーザ定義グループ「作成者」 -->
		<UserDefGroup key="formCreationUserDefinitionGroupPlugIn" id="-11000">
			<supportGetSearchCondition>true</supportGetSearchCondition>
			<supportGetUserList>true</supportGetUserList>
			<nameList>
				<name lang="JA" langname="帳票作成者" />
