<?xml version="1.0" encoding="UTF-8"?>
<UserDefGroupConf>

	<!-- ====================================================================================== -->
	<!-- ユーザー定義グループコンフィグレーション                                               -->
	<!-- ドキュメント管理ではFW1とFW2が混在しているため一部PlugInラッパークラスを使用している。 -->
	<!-- ====================================================================================== -->

	<UserDefGroups>

		<!-- ===================== -->
		<!-- EIMANAGER Framework   -->
		<!-- ===================== -->

		<!-- 作成者 -->
		<UserDefGroup key="creatorUserDefinitionGroupPlugInWrapper" id="-1001">
			<supportGetSearchCondition>true</supportGetSearchCondition>
			<supportGetUserList>true</supportGetUserList>
			<nameList>
				<name lang="JA" langname="作成者" />
				<name lang="EN" langname="Creator" />
			</nameList>
		</UserDefGroup>

		<!-- 担当者 -->
		<UserDefGroup key="assignedPersonsUserDefinitionGroupPlugInWrapper" id="-1002">
			<supportGetSearchCondition>true</supportGetSearchCondition>
			<supportGetUserList>true</supportGetUserList>
			<nameList>
				<name lang="JA" langname="担当者" />
				<name lang="EN" langname="Assigned Persons" />
			</nameList>
		</UserDefGroup>

		<!-- ===================== -->
		<!-- ドキュメント管理      -->
		<!-- ===================== -->

		<!-- ユーザ定義グループ「ワークスペース管理者（ごみ箱表示用）」 -->
		<UserDefGroup key="workSpaceAdminRecycleUserDefGroupPlugIn" id="-12000">
			<supportGetSearchCondition>true</supportGetSearchCondition>
			<supportGetUserList>true</supportGetUserList>
			<nameList>
				<name lang="JA" langname="ワークスペース管理者（ごみ箱表示用）" />
