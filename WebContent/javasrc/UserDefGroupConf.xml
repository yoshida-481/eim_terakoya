			</nameList>
		</UserDefGroup>

		<!-- ユーザ定義グループ「担当者」 -->
		<UserDefGroup key="responsibleUserDefinitionGroupPlugIn" id="-11001">
			<supportGetSearchCondition>true</supportGetSearchCondition>
			<supportGetUserList>true</supportGetUserList>
			<nameList>
				<name lang="JA" langname="帳票担当者" />
				<name lang="EN" langname="Form Responsible User" />
			</nameList>
		</UserDefGroup>

		<!-- ユーザ定義グループ「任意ユーザ属性」 -->
