		<UserDefGroup key="roleInChargeUserDefinitionGroupPlugInWrapper" id="-21001">
			<supportGetSearchCondition>true</supportGetSearchCondition>
			<supportGetUserList>true</supportGetUserList>
			<nameList>
				<name lang="JA" langname="担当役割" />
				<name lang="EN" langname="Role In Charge" />
			</nameList>
		</UserDefGroup>

