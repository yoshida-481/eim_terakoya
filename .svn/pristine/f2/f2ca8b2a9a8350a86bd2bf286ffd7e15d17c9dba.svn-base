<?xml version="1.0" encoding="UTF-8"?>

<!-- 操作履歴削除バッチの設定 -->
<deleteOpeHistConfList>
    <deleteOpeHistConfigration>

    	<!-- 操作履歴のDB上の保管期間(単位：日) ※範囲は1～2147483647 -->
    	<DBSaveTerm value = "1000" />

    	<!-- CSVファイルの出力先ディレクトリ(最後尾の「/」まで指定) -->
    	<CSVDirectory value =  "./" />

    	<!-- CSVファイルのベース名([出力先ディレクトリ][ベース名]_YYYY-MM-DD_hhmmss.logで出力) -->
    	<CSVFileNameBase value = "OperationHistory" />

    	<!-- CSVファイル出力の可否(on:CSV出力して削除, off:CSV出力せずに削除) -->
    	<CSVOutput value = "on" />

    </deleteOpeHistConfigration>
</deleteOpeHistConfList>