package batch.migration.domain;

/**
 * CSV出力項目保持用ドメイン
 * @since Ver
 */
public class ExportCSVDomain implements Cloneable{

	/** ID */
	private long id = 0;

	/** 名称 */
	private String name = null;

	/** パス */
	private String path = null;

	/** サイズ */
	private long size = 0;

	/** 結果 */
	private String result = null;

	/**
	 * Idを取得します。
	 * @return Id
	 * @since Ver1.0
	 */
	public long getId() {
	    return id;
	}

	/**
	 * Idを設定します。
	 * @param int Id
	 * @since Ver1.0
	 */
	public void setId(long id) {
	    this.id = id;
	}

	/**
	 * 名称を取得します。
	 * @return 名称
	 * @since Ver1.0
	 */
	public String getName() {
	    return name;
	}

	/**
	 * 名称を設定します。
	 * @param String 名称
	 * @since Ver1.0
	 */
	public void setName(String name) {
	    this.name = name;
	}

	/**
	 * パスを取得します。
	 * @return パス
	 * @since Ver1.0
	 */
	public String getPath() {
	    return path;
	}

	/**
	 * パスを設定します。
	 * @param int パス
	 * @since Ver1.0
	 */
	public void setPath(String path) {
	    this.path = path;
	}

	/**
	 * サイズを取得します。
	 * @return サイズ
	 * @since Ver1.0
	 */
	public long getSize() {
	    return size;
	}

	/**
	 * サイズを設定します。
	 * @param int サイズ
	 * @since Ver1.0
	 */
	public void setSize(long size) {
	    this.size = size;
	}

	/**
	 * 結果を取得します。
	 * @return 結果
	 * @since Ver1.0
	 */
	public String getResult() {
	    return result;
	}

	/**
	 * 結果を設定します。
	 * @param String 結果
	 * @since Ver1.0
	 */
	public void setResult(String result) {
	    this.result = result;
	}
}
