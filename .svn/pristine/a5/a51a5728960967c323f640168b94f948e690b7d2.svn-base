package jp.co.ctc_g.eim.app.document.business.domain.search.criteria;

import jp.co.ctc_g.eim.framework2.business.domain.criteria.RangeCriteria;

/**
 * オブジェクト検索における属性範囲指定条件クラスです。<br>
 * 整数型、実数型、日付型に対応します。
 * @param <T>
 */
public class ContentSearchAttributeRangeCriteria<T> extends ContentSearchAttributeCriteria {

	/** 範囲条件 */
	private RangeCriteria<T> range = null;

	public RangeCriteria<T> getRange() {
		return range;
	}

	public void setRange(RangeCriteria<T> range) {
		this.range = range;
	}
}
