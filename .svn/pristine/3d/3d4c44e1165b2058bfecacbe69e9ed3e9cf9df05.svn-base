package jp.co.ctc_g.eim.admin.business.domain;

import java.io.Serializable;

/**
 * キャッシュモニタードメイン
 *
 */
public class CacheMonitorNodeDomain implements Serializable {

	/** ノードID */
	private long id;

	/** ノード名称 */
	private String name;

	/** ノードのスキーム */
	private String scheme;

	/** ノードのホスト（ホスト名またはIP） */
	private String host;

	/** ノードのポート番号 */
	private String port;

	/** ノードのパス */
	private String path;


	/**
	 * ノードIDを取得します。
	 * @return ノードID
	 */
	public long getId() {
		return id;
	}

	/**
	 * ノードIDを設定します。
	 * @param id ノードID
	 */
	public void setId(long id) {
		this.id = id;
	}

	/**
	 * ノード名称を取得します。
	 * @return ノード名称
	 */
	public String getName() {
		return name;
	}

	/**
	 * ノード名称を設定します。
	 * @param name ノード名称
	 */
	public void setName(String name) {
		this.name = name;
	}

	/**
	 * ノードのスキームを取得します。
	 * @return ノードのスキーム
	 */
	public String getScheme() {
		return scheme;
	}

	/**
	 * ノードのスキームを設定します。
	 * @param scheme ノードのスキーム
	 */
	public void setScheme(String scheme) {
		this.scheme = scheme;
	}

	/**
	 * ノードのホスト（ホスト名またはIP）を取得します。
	 * @return ノードのホスト
	 */
	public String getHost() {
		return host;
	}

	/**
	 * ノードのホスト（ホスト名またはIP）を設定します。
	 * @param host ノードのホスト
	 */
	public void setHost(String host) {
		this.host = host;
	}

	/**
	 * ノードのポート番号を取得します。
	 * @return ノードのポート番号
	 */
	public String getPort() {
		return port;
	}

	/**
	 * ノードのポート番号を設定します。
	 * @param port ノードのポート番号
	 */
	public void setPort(String port) {
		this.port = port;
	}

	/**
	 * ノードのパスを取得します。
	 * @return ノードのパス
	 */
	public String getPath() {
		return path;
	}

	/**
	 * ノードのパスを設定します。
	 * @param path ノードのパス
	 */
	public void setPath(String path) {
		this.path = path;
	}

}
