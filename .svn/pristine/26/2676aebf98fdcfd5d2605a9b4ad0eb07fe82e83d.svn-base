	<!-- セッションタイムアウトチェック -->
	<filter>
		<filter-name>SessionTimeoutCheckFilter</filter-name>
		<filter-class>
			jp.co.ctc_g.eim.framework.presentation.web.filter.SessionTimeoutCheckFilter
		</filter-class>
		<init-param>
			<param-name>ignoreUrl</param-name>
			<param-value>/login.do,/cache-monitor/caches.mvc,/cache-monitor/entries/search.mvc</param-value>
		</init-param>
		<init-param>
			<param-name>sessionErrorPage</param-name>
			<param-value>/servlet/XmlErrorMessageServlet</param-value>
		</init-param>
	</filter>
	<filter>
		<filter-name>SessionTimeoutCheckDownloadFilter</filter-name>
		<filter-class>
			jp.co.ctc_g.eim.framework.presentation.web.filter.SessionTimeoutCheckFilter
		</filter-class>
		<init-param>
			<param-name>sessionErrorPage</param-name>
			<param-value>/servlet/HtmlErrorMessageServlet</param-value>
		</init-param>
	</filter>
	<filter>
		<filter-name>SessionTimeoutCheckFilterForRestAPI</filter-name>
		<filter-class>
			jp.co.ctc_g.eim.framework2.presentation.web.filter.SessionTimeoutCheckFilterForRestAPI
		</filter-class>
		<init-param>
			<param-name>ignoreUrl</param-name>
			<param-value>/apis/languages</param-value>
		</init-param>
	</filter>
