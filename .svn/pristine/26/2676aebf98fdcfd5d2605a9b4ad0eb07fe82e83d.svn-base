	<filter-mapping>
		<filter-name>WebDAVSessionTimeoutCheckFilter2</filter-name>
		<url-pattern>/WebDAVServlet/*</url-pattern>
	</filter-mapping>

	<!-- アクセスキーチェック -->
	<filter-mapping>
		<filter-name>AccessKeyCheckFilter</filter-name>
		<url-pattern>/cache-monitor/*</url-pattern>
	</filter-mapping>

