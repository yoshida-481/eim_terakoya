	<!-- セッションタイムアウトチェック -->
	<filter-mapping>
		<filter-name>SessionTimeoutCheckFilter</filter-name>
		<url-pattern>*.do</url-pattern>
		<url-pattern>*.mvc</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>SessionTimeoutCheckFilterForRestAPI</filter-name>
		<url-pattern>/apis/*</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>SessionTimeoutCheckDownloadFilter</filter-name>
		<url-pattern>/rest/search/file_download/*</url-pattern>
	</filter-mapping>
	<!--<filter-mapping>
		<filter-name>SessionTimeoutCheckFilter2</filter-name>
		<url-pattern>*.do</url-pattern>
	</filter-mapping>-->

