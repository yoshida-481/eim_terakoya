package jp.co.ctc_g.eim.app.document.common.enumeration;

/**
 * 【ドキュメントAPI】
 * アクセス権限列挙型
 */
public enum DocumentAccessRoleTypeEnum {

	/** 書き込み */
	WRITE("WRITE"),
	/** ステータス変更 */
	STATUS_CHANGE("STATUS_CHANGE"),
	/** 常時読取 */
	ALWAYS_READ("ALWAYS_READ"),
	/** 公開読取 */
	PUBLIC_READ("PUBLIC_READ");

	/**  設定値*/
	private final String value;

	/**
	 * コンストラクタ<br>
	 * @param value アクセス権限の設定値
	 */
	private DocumentAccessRoleTypeEnum(String value) {
		this.value = value;
	}

	/**
	 * アクセス権限の設定値を取得します。
	 * @return アクセス権限の設定値
	 */
	public String getValue() {
		return value;
	}


}
