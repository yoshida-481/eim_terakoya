<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="
			http://www.springframework.org/schema/beans
			http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- fileAccessService2ForAPI（オブジェクト更新時更新者/更新日を更新する） -->
	<bean id="fileAccessService2ForAPI" parent="fileAccessService2">
		<property name="fileDao">
			<ref bean="fileDao2ForAPI"/>
		</property>
	</bean>

	<!-- fileAccessService2ForAPI（オブジェクト更新時更新者/更新日を更新しない） -->
	<bean id="fileAccessService2ForAPINotUpdateModificationInfo" parent="fileAccessService2ForAPI">
		<property name="fileDao">
			<ref bean="fileDao2ForAPINotUpdateModificationInfo"/>
		</property>
	</bean>

	<!-- objectService2ForAPI（オブジェクト更新時更新者/更新日を更新する） -->
	<bean id="objectService2ForAPI" parent="objectService2">
		<property name="objectDao">
			<ref bean="objectDao2ForAPI"/>
		</property>
	</bean>

	<!-- objectService2ForAPI（オブジェクト更新時更新者/更新日を更新しない） -->
	<bean id="objectService2ForAPINotUpdateModificationInfo" parent="objectService2ForAPI">
		<property name="objectDao">
			<ref bean="objectDao2ForAPINotUpdateModificationInfo"/>
		</property>
	</bean>

	<!-- objectService2ForAPISuppressGetObjectEventAspect（取得系のオブジェクト操作イベントアスペクトを抑制します） -->
	<bean id="objectService2ForAPISuppressGetObjectEventAspect" parent="objectService2ForAPI" >
	</bean>

	<!-- relationService2ForAPI -->
	<bean id="relationService2ForAPI" class="jp.co.ctc_g.eim.framework2.business.service.impl.RelationServiceImpl" >
		<property name="relationDao">
			<ref bean="relationDao2ForAPI"/>
		</property>
	</bean>

	<!-- relationService2ForAPISuppressGetRelationEventAspect（取得系のリレーション操作イベントアスペクトを抑制します） -->
	<bean id="relationService2ForAPISuppressGetRelationEventAspect" parent="relationService2ForAPI" >
	</bean>
</beans>
