package jp.co.ctc_g.eim.app.document.business.domain;

import java.util.ArrayList;
import java.util.Date;
import java.util.List;

import com.fasterxml.jackson.annotation.JsonIgnoreProperties;

import jp.co.ctc_g.eim.framework2.business.domain.entity.AttributeTypeDomain;

/**
 * 
 */
@JsonIgnoreProperties(ignoreUnknown = true)
public class DocumentFormAttributeDomain {
	
	/** 属性タイプ */
	private AttributeTypeDomain attributeType = null;
	
	/** 数値型属性値リスト */
	private List<Long> longList = new ArrayList<Long>();

	/** 日付型属性値リスト */
	private List<Date> dateList = new ArrayList<Date>();

	/** 実数型属性値リスト */
	private List<Double> doubleList = new ArrayList<Double>();

	/** テキスト型属性値リスト */
	private List<String> textList = new ArrayList<String>();
	
	/** 文字列型属性値リスト */
	private List<String> stringList = new ArrayList<String>();
	
	
	public AttributeTypeDomain getAttributeType() {
		return attributeType;
	}

	public void setAttributeType(AttributeTypeDomain attributeType) {
		this.attributeType = attributeType;
	}

	public List<Long> getLongList() {
		return longList;
	}

	public void setLongList(List<Long> longList) {
		this.longList = longList;
	}

	public List<Date> getDateList() {
		return dateList;
	}

	public void setDateList(List<Date> dateList) {
		this.dateList = dateList;
	}

	public List<Double> getDoubleList() {
		return doubleList;
	}

	public void setDoubleList(List<Double> doubleList) {
		this.doubleList = doubleList;
	}

	public List<String> getTextList() {
		return textList;
	}

	public void setTextList(List<String> textList) {
		this.textList = textList;
	}

	public List<String> getStringList() {
		return stringList;
	}

	public void setStringList(List<String> stringList) {
		this.stringList = stringList;
	}
	
}
