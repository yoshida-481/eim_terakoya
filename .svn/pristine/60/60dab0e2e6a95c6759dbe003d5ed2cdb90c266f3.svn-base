package common.enumeration;

/**
 *傾きを扱う列挙型クラス
 */
public enum RotationEnum {
	/** 0度 */
	ROTATION_0(0),
	/** 90度 */
	ROTATION_90(90),
	/** 180度 */
	ROTATION_180(180),
	/** 270度 */
	ROTATION_270(270);

	private final int value;
	
	private RotationEnum(int value) {
		this.value = value;
	}
	/**
	 * enumで指定した傾きを返却します。
	 * @return 傾き
	 */
	public int getValue() {
		return this.value;
	}
}