<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xmlns:jee="http://www.springframework.org/schema/jee"
		xsi:schemaLocation="
			http://www.springframework.org/schema/beans
			http://www.springframework.org/schema/beans/spring-beans.xsd
			http://www.springframework.org/schema/tx
			http://www.springframework.org/schema/tx/spring-tx.xsd
			http://www.springframework.org/schema/aop
			http://www.springframework.org/schema/aop/spring-aop.xsd
			http://www.springframework.org/schema/jee
			http://www.springframework.org/schema/jee/spring-jee.xsd">

	<!-- PlugIn実装 -->
	<!-- ======================================================================================== -->
	<bean id="UserDefDummy" class="jp.co.ctc_g.eim.app.document.business.service.impl.UserDefDummyPlugInImpl" />

	<!-- 承認依頼通知先 -->
	<bean id="userDefAddressRequestApprovalPlugIn" class="jp.co.ctc_g.eim.app.document.business.service.impl.UserDefAddressRequestApprovalPlugInImpl" >
		<property name="statusDao">
			<ref bean="statusDao"/>
		</property>
	</bean>

	<!-- 承認通知先 -->
	<bean id="userDefAddressApprovedPlugIn" class="jp.co.ctc_g.eim.app.document.business.service.impl.UserDefAddressApprovedPlugInImpl" >
		<property name="eventHistoryDao">
			<ref bean="eventHistoryDaoWithFromStatus"/>
		</property>
	</bean>

	<!-- 差戻し通知先 -->
	<bean id="userDefAddressRejectionPlugIn" class="jp.co.ctc_g.eim.app.document.business.service.impl.UserDefAddressRejectionPlugInImpl" >
		<property name="statusDao">
			<ref bean="statusDao"/>
		</property>
		<property name="eventHistoryDao">
			<ref bean="eventHistoryDaoWithTypeAndFromStatus"/>
		</property>
		<property name="userDefAddressApprovedPlugIn">
			<ref bean="userDefAddressApprovedPlugIn"/>
		</property>
	</bean>

	<!-- 承認依頼取消通知先 -->
	<bean id="userDefAddressCancelApprovalPlugIn" class="jp.co.ctc_g.eim.app.document.business.service.impl.UserDefAddressCancelApprovalPlugInImpl" >
		<property name="eventHistoryDao">
			<ref bean="eventHistoryDaoWithoutAttribute"/>
		</property>
		<property name="statusDao">
			<ref bean="statusDao"/>
		</property>
		<property name="assignDao">
			<ref bean="assignDao"/>
		</property>
	</bean>

	<!-- 公開通知先 -->
	<bean id="userDefAddressPublishPlugIn" class="jp.co.ctc_g.eim.app.document.business.service.impl.UserDefAddressPublishPlugInImpl" >
	</bean>

	<!-- 公開取消通知先 -->
	<bean id="userDefAddressPublishCancelPlugIn" class="jp.co.ctc_g.eim.app.document.business.service.impl.UserDefAddressPublishCancelPlugInImpl" >
		<property name="eventHistoryDao">
			<ref bean="eventHistoryDaoWithFromStatus"/>
		</property>
	</bean>

	<!-- ワークスペース管理者(ごみ箱表示用)（ユーザ定義グループ）プラグイン -->
	<bean id="workSpaceAdminRecycleUserDefGroupPlugIn" class="jp.co.ctc_g.eim.app.document.business.service.impl.WorkSpaceAdminRecycleUserDefGroupPlugInImpl" >
		<property name="objectTypeService">
			<ref bean="objectTypeServiceLight2"/>
		</property>
		<property name="attributeTypeService">
			<ref bean="attributeTypeServiceLight2"/>
		</property>
		<property name="relationService">
			<ref bean="relationService2"/>
		</property>
		<property name="userService">
			<ref bean="userService2"/>
		</property>
	</bean>

	<!-- ワークスペース管理者(ごみ箱配下表示用)（ユーザ定義グループ）プラグイン -->
	<bean id="workSpaceAdminInRecycleUserDefGroupPlugIn" class="jp.co.ctc_g.eim.app.document.business.service.impl.WorkSpaceAdminInRecycleUserDefGroupPlugInImpl" >
		<property name="objectTypeService">
			<ref bean="objectTypeServiceLight2"/>
		</property>
		<property name="attributeTypeService">
			<ref bean="attributeTypeServiceLight2"/>
		</property>
		<property name="objectService">
			<ref bean="objectService2"/>
		</property>
		<property name="userService">
			<ref bean="userService2"/>
		</property>
	</bean>


	<!--
		バッチ関連
	 -->
	<!-- PDFセキュリティ設定オブジェクトの登録者 -->
	<bean id="IMPL.USER.TO.SIGNATURE.REGST.USER" class="jp.co.ctc_g.eim.app.document.business.service.impl.UserDefCUserOfPDFSecuritySettingObjectPlugInImpl"/>

	<!-- オブジェクトの作成者(CUSER) -->
	<bean id="IMPL.USER.TO.OBJECT.CUSER" class="jp.co.ctc_g.eim.app.document.business.service.impl.UserDefLoginUserPlugInImpl"/>

	<!-- PDF結合オブジェクトの登録者 -->
	<bean id="IMPL.USER.TO.PDFJOINOBJECT.REGST.USER" class="jp.co.ctc_g.eim.app.document.business.service.impl.UserDefRegistUserOfPDFJoinUserAttributePlugInImpl"/>

	<!-- メールアドレスからユーザ情報取得 -->
	<bean id="IMPL.USER.TO.MAILADDRESS.FROM.USER" class="jp.co.ctc_g.eim.app.document.business.service.impl.UserDefMailAddressFromUserPlugInImpl"/>

	<!-- 公開通知オブジェクトの通知先 -->
	<bean id="IMPL.USER.TO.NOTIFYUSER.OF.PUBLISHMAIL" class="jp.co.ctc_g.eim.app.document.business.service.impl.UserDefNotifyToUserOfPublichMailPlugInImpl"/>

	<!-- 公開ファイル比較オブジェクトの作成者(CUSER) -->
	<bean id="IMPL.USER.TO.PDFCOMPARE.OBJECT.CUSER" class="jp.co.ctc_g.eim.app.document.business.service.impl.UserDefCUserOfPDFCompareObjectPlugInImpl"/>

	<!-- 公開ファイル結合オブジェクトの作成者(CUSER) -->
	<bean id="IMPL.USER.TO.JOIN.OBJECT.CUSER" class="jp.co.ctc_g.eim.app.document.business.service.impl.UserDefCUserOfPDFJoinObjectPlugInImpl"/>

	<!-- 有効期限切れドキュメント削除通知先 -->
	<bean id="IMPL.USER.TO.MAILADDRESS.DISPOSE.EXPIRED.DOCS.USER" class="jp.co.ctc_g.eim.app.document.business.service.impl.UserDefAddressDisposeExpiredDocsPlugInImpl" />

	<!-- 有効期限事前通知先 -->
	<bean id="IMPL.USER.TO.MAILADDRESS.EXPIRATION.DATE.NOTICE.USER" class="jp.co.ctc_g.eim.app.document.business.service.impl.UserDefAddressDisposeExpiredDocsPlugInImpl" />


</beans>
