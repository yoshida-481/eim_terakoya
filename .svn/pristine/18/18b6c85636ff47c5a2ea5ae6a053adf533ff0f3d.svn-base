<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="
			http://www.springframework.org/schema/beans
			http://www.springframework.org/schema/beans/spring-beans.xsd">


	<!-- Dao実装 -->
	<!-- ======================================================================================== -->
	<bean id="NamespaceDao" class="jp.co.ctc_g.eim.admin.integration.dao.impl.xml.NamespaceDaoImpl" init-method="init">
		<!-- 設定例：/NamespaceConf.xml -->
		<property name="namespaceConfFilePath">
			<null/>
		</property>
	</bean>
	
	<!-- adminUserDao -->
	<bean id="adminUserDao" class="jp.co.ctc_g.eim.admin.integration.dao.impl.AdminUserDaoImpl">
		<property name="userDao">
			<ref bean="userDaoForAdminUser"/>
		</property>
	</bean>
	<!-- attachementFileDao -->
	<bean id="attachementFileDao" class="jp.co.ctc_g.eim.framework2.integration.dao.impl.FileDaoImpl" >
		<property name="itemListForSingleTargetMethod">
			<list>
				<value>DIRECTORY</value>
				<value>FORMAT</value>
			</list>
		</property>
		<property name="itemListForMultiTargetMethod">
			<list>
				<value>DIRECTORY</value>
				<value>FORMAT</value>
			</list>
		</property>
		<!-- 更新時に更新日時/更新者を更新するか -->
		<property name="updatedModificationInfo">
			<value>${UPDATED_MODIFICATION_INFO_FW2}</value>
		</property>
		<property name="directoryDao">
			<ref bean="directoryDao2"/>
		</property>
		<property name="formatDao">
			<ref bean="formFormatDao2"/>
		</property>
	</bean>

	<!-- ドキュメントフォームオブジェクトDAO -->
	<bean id="documentFormObjectDao2ForAttributeAll" class="jp.co.ctc_g.eim.framework2.integration.dao.impl.ObjectDaoImpl" >
		<!-- null（全て取得する） -->
		<property name="attributeTypeListForSingleTargetMethod">
			<null/>
		</property>
		<property name="attributeTypeListForMultiTargetMethod">
			<null/>
		</property>
		<!-- null（全て取得する） -->
		<property name="itemListForSingleTargetMethod">
			<null/>
		</property>
		<property name="itemListForMultiTargetMethod">
			<null/>
		</property>
		<property name="isTakeStatusListForMultiTarget" value="false" />
		<property name="isTakeStatusListForSingleTarget" value="true" />
		<property name="limit" value="0" />
		<property name="bulkCount" value="200" />
		<property name="limitCondition" value="false" />
		<property name="statusDao" ref="statusDao2" />
		<property name="workflowDao" ref="workflowDao2" />
		<property name="attributeLoadingQueryTypeEnum" value="AUTO" />
		<!-- 
				getList(SearchSelectObject, SearchLimitCountCondition)において、
					取得項目および属性項目の絞り込みの挙動を切り替えるフラグ
				・true:引数⇒applicationContextのプロパティ⇒実装の順で参照します。
				・false:引数⇒実装の順で参照します。
						REVISION_GROUP_IDについては、applicationContextのみ参照します。
				本設定ファイルでは、trueとします。
				コメントアウトした場合はfalseと同じ挙動となります。
		-->
		<property name="enabledListForGetListSelectTarget">
			<value>true</value>
		</property>
		<property name="isLoadProperty" value="false" />
		<!-- 更新時に更新日時/更新者を更新するか -->
		<property name="updatedModificationInfo" value="false" />
		<!-- オブジェクト型の属性は循環参照チェックしない -->
		<property name="enabledObjectAttributeCirculationCheck" value="false" />
	</bean>
	
	<!-- ドキュメントオブジェクトDAO -->
	<bean id="documentObjectDao2" class="jp.co.ctc_g.eim.framework2.integration.dao.impl.ObjectDaoImpl" >
		<!-- null（全て取得する） -->
		<property name="attributeTypeListForSingleTargetMethod">
			<null/>
		</property>
		<!-- null（全て取得する） -->
		<property name="attributeTypeListForMultiTargetMethod">
			<null/>
		</property>
		<property name="itemListForSingleTargetMethod">
			<null/>
		</property>
		<property name="itemListForMultiTargetMethod">
			<null/>
		</property>
		<property name="limit">
			<value>1000</value>
		</property>
		<property name="bulkCount">
			<value>200</value>
		</property>
		<property name="limitCondition">
			<value>false</value>
		</property>
		<property name="statusDao">
			<ref bean="statusDaoLight2"/>
		</property>
		<property name="workflowDao">
			<ref bean="workflowDao2"/>
		</property>
		<property name="objectTypeDao">
			<ref bean="objectTypeDao2"/>
		</property>
		<property name="attributeLoadingQueryTypeEnum">
			<value>AUTO</value>
		</property>
		<!-- オブジェクト複数取得メソッドでステータスリストを取得するかしないかを指定 -->
		<property name="isTakeStatusListForMultiTarget">
			<value>false</value>
		</property>
		<!-- オブジェクト単数取得メソッドでステータスリストを取得するかしないかを指定 -->
		<property name="isTakeStatusListForSingleTarget">
			<value>false</value>
		</property>
		<!-- 
				getList(SearchSelectObject, SearchLimitCountCondition)において、
					取得項目および属性項目の絞り込みの挙動を切り替えるフラグ
				・true:引数⇒applicationContextのプロパティ⇒実装の順で参照します。
				・false:引数⇒実装の順で参照します。
						REVISION_GROUP_IDについては、applicationContextのみ参照します。
				本設定ファイルでは、trueとします。
				コメントアウトした場合はfalseと同じ挙動となります。
		-->
		<property name="enabledListForGetListSelectTarget">
			<value>true</value>
		</property>
		<property name="isLoadProperty">
			<value>false</value>
		</property>
		<property name="enabledObjectAttributeCirculationCheck" value="false" /> <!-- オブジェクト型の属性は循環参照チェックしない -->
		<!-- 更新時に更新日時/更新者を更新するか -->
		<property name="updatedModificationInfo">
			<value>${UPDATED_MODIFICATION_INFO_FW2}</value>
		</property>
	</bean>
	
	<!-- オブジェクトタイプDAO（添付ファイルオブジェクト用） -->
	<bean id="dcoumentFormTempFileObjectObjectTypeDao" parent="objectTypeDao2" >
		<property name="itemListForSingleTargetMethod">
			<list>
			</list>
		</property>
	</bean>
	
	<!-- 回付状況確認用オブジェクトDao -->
	<bean id="objectDaoForCirculation" class="jp.co.ctc_g.eim.framework2.integration.dao.impl.ObjectDaoImpl" >
		<!-- null（全て取得する） -->
		<property name="attributeTypeListForSingleTargetMethod">
			<null/>
		</property>
		<!-- null（全て取得する） -->
		<property name="attributeTypeListForMultiTargetMethod">
			<null/>
		</property>
		<property name="itemListForSingleTargetMethod">
			<null/>
		</property>
		<property name="itemListForMultiTargetMethod">
			<null/>
		</property>
		<property name="limit">
			<value>0</value>
		</property>
		<property name="bulkCount">
			<value>200</value>
		</property>
		<property name="limitCondition">
			<value>false</value>
		</property>
		<property name="statusDao">
			<ref bean="statusDaoLight2"/>
		</property>
		<property name="workflowDao">
			<ref bean="workflowDao2"/>
		</property>
		<property name="objectTypeDao">
			<ref bean="objectTypeDao2"/>
		</property>
		<property name="attributeLoadingQueryTypeEnum">
			<value>AUTO</value>
		</property>
		<!-- オブジェクト複数取得メソッドでステータスリストを取得するかしないかを指定 -->
		<property name="isTakeStatusListForMultiTarget">
			<value>true</value>
		</property>
		<!-- オブジェクト単数取得メソッドでステータスリストを取得するかしないかを指定 -->
		<property name="isTakeStatusListForSingleTarget">
			<value>true</value>
		</property>
		<!-- 
				getList(SearchSelectObject, SearchLimitCountCondition)において、
					取得項目および属性項目の絞り込みの挙動を切り替えるフラグ
				・true:引数⇒applicationContextのプロパティ⇒実装の順で参照します。
				・false:引数⇒実装の順で参照します。
						REVISION_GROUP_IDについては、applicationContextのみ参照します。
				本設定ファイルでは、trueとします。
				コメントアウトした場合はfalseと同じ挙動となります。
		-->
		<property name="enabledListForGetListSelectTarget">
			<value>true</value>
		</property>
		<property name="isLoadProperty">
			<value>false</value>
		</property>
		<!-- 更新時に更新日時/更新者を更新するか -->
		<property name="updatedModificationInfo">
			<value>${UPDATED_MODIFICATION_INFO_FW2}</value>
		</property>
	</bean>
	
	<!-- 回付状況確認用オブジェクトタイプDAO -->
	<bean id="objectTypeDaoForCirculation" class="jp.co.ctc_g.eim.framework2.integration.dao.impl.ObjectTypeDaoImpl" >
		<property name="itemListForSingleTargetMethod">
			<list>
				<value>CHILD_LIST</value>
			</list>
		</property>
		<property name="itemListForMultiTargetMethod">
			<list>
				<value>CHILD_LIST</value>
			</list>
		</property>
		<property name="parentRecursiveLevel">
			<value>1</value>
		</property>
		<property name="childRecursiveLevel">
			<value>0</value>
		</property>
		<!-- 更新時に更新日時/更新者を更新するか -->
		<property name="updatedModificationInfo">
			<value>${UPDATED_MODIFICATION_INFO_FW2}</value>
		</property>
		<!-- 設定無し -->
		<property name="attributeTypeDao">
			<null/>
		</property>
		<!-- 設定無し -->
		<property name="formatDao">
			<null/>
		</property>
		<property name="objectDao">
			<ref bean="objectDaoLight2"/>
		</property>
		<!-- 設定無し -->
		<property name="workflowDao">
			<null/>
		</property>
		<!-- 設定無し -->
		<property name="sequenceDao">
			<null/>
		</property>
	</bean>
	
	<!-- 回付状況確認用ステータスDAO -->
	<bean id="statusDaoForCirculation" class="jp.co.ctc_g.eim.framework2.integration.dao.impl.StatusDaoImpl" >
		<!-- null（全て取得する） -->
		<property name="attributeTypeListForSingleTargetMethod">
			<null/>
		</property>
		<!-- null（全て取得する） -->
		<property name="attributeTypeListForMultiTargetMethod">
			<null/>
		</property>
		<property name="itemListForSingleTargetMethod">
			<list>
				<value>TYPE</value>
				<value>CREATION_USER</value>
				<value>MODIFICATION_USER</value>
				<value>ASSIGNMENT_LIST</value>
			</list>
		</property>
		<property name="itemListForMultiTargetMethod">
			<list>
				<value>ASSIGNMENT_LIST</value>
			</list>
		</property>
		<!-- 更新時に更新日時/更新者を更新するか -->
		<property name="updatedModificationInfo">
			<value>${UPDATED_MODIFICATION_INFO_FW2}</value>
		</property>
		<property name="assignmentDao">
			<ref bean="assignmentDaoLight2"/>
		</property>
		<property name="assignmentPlanDao">
			<ref bean="assignmentPlanDao2"/>
		</property>
		<property name="assignmentEntryDao">
			<ref bean="assignmentEntryDao2"/>
		</property>
		<property name="statusTypeDao">
			<ref bean="statusTypeDaoLight2"/>
		</property>
		<property name="userDao">
			<ref bean="userDaoLight2"/>
		</property>
		<property name="userDefinitionGroupDao">
			<ref bean="userDefinitionGroupDao2"/>
		</property>
		<property name="systemFunctionDao">
			<ref bean="systemFunctionDao2"/>
		</property>
	</bean>
	
	<!-- オブジェクトタイプDAO（チェックイン用） -->
	<bean id="checkinObjectTypeDao" parent="objectDao2" >
		<property name="enabledObjectAttributeCirculationCheck" value="false" /> <!-- オブジェクト型の属性は循環参照チェックしない -->
	</bean>

	<!-- PDFバッチ用オブジェクトDao -->
	<bean id="objectDaoForPDFBatch" class="jp.co.ctc_g.eim.app.document.integration.dao.impl.PDFBatchObjectDaoImpl" >
	</bean>

</beans>