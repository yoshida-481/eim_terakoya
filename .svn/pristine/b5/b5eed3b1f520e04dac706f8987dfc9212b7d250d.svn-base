<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="
			http://www.springframework.org/schema/beans
			http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- ============================== -->
	<!-- ユーザー定義グループプラグイン -->
	<!-- ============================== -->

	<!-- 担当役割プラグイン -->
	<bean id="roleInChargeUserDefinitionGroupPlugIn" class="jp.co.ctc_g.eim.app.task.business.plugin.impl.RoleInChargeUserDefinitionGroupPlugInImpl">
		<property name="objectService" ref="objectService2"/>
		<property name="memberEntryService" ref="memberEntryService2WithUserListRecursive"/>
		<property name="attributeTypeService" ref="attributeTypeServiceLight2"/>
	</bean>

	<!-- 担当役割(未着手)プラグイン -->
	<bean id="roleInChargeInNewStatusUserDefinitionGroupPlugIn" parent="roleInChargeUserDefinitionGroupPlugIn">
		<property name="enableBaseStatusTypeIdList">
			<list>
				<!-- 未着手 -->
				<value>-23001</value>
			</list>
		</property>
	</bean>

	<!-- ====================================== -->
	<!-- ユーザー定義グループプラグインラッパー -->
	<!-- (FW1から利用する場合)                  -->
	<!-- ====================================== -->

	<!-- 担当役割プラグインラッパー -->
	<bean id="roleInChargeUserDefinitionGroupPlugInWrapper" class="jp.co.ctc_g.eim.framework.business.service.impl.UserDefGroupPlugInWrapper">
		<property name="plugIn2" ref="roleInChargeUserDefinitionGroupPlugIn"/>
	</bean>

	<!-- 担当役割(未着手)プラグインラッパー -->
	<bean id="roleInChargeInNewStatusUserDefinitionGroupPlugInWrapper" class="jp.co.ctc_g.eim.framework.business.service.impl.UserDefGroupPlugInWrapper">
		<property name="plugIn2" ref="roleInChargeInNewStatusUserDefinitionGroupPlugIn"/>
	</bean>

</beans>
