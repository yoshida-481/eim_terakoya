<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="
			http://www.springframework.org/schema/beans
			http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- ============== -->
	<!-- 検索FW用Dao    -->
	<!-- ============== -->

	<!-- ページ指定ファイルダウンロード用FileDao -->
	<bean id="fileDaoForSearch" parent="fileDao2" >
		<property name="itemListForMultiTargetMethod">
			<list>
				<value>DIRECTORY</value>
				<value>FORMAT</value>
			</list>
		</property>
	</bean>

	<!-- 更新通知アドバイス用オブジェクトタイプDAO -->
	<bean id="objectTypeDaoForSearch" parent="objectTypeDao2" >
		<property name="itemListForSingleTargetMethod">
			<list>
				<value>CHILD_LIST</value>
			</list>
		</property>
		<property name="itemListForMultiTargetMethod">
			<list>
				<value>CHILD_LIST</value>
			</list>
		</property>
		<property name="parentRecursiveLevel">
			<value>1</value>
		</property>
		<property name="childRecursiveLevel">
			<value>0</value>
		</property>
	</bean>

</beans>