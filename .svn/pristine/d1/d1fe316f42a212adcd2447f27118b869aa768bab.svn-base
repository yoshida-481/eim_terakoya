<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xmlns:jee="http://www.springframework.org/schema/jee"
		xsi:schemaLocation="
			http://www.springframework.org/schema/beans
			http://www.springframework.org/schema/beans/spring-beans.xsd
			http://www.springframework.org/schema/tx
			http://www.springframework.org/schema/tx/spring-tx.xsd
			http://www.springframework.org/schema/aop
			http://www.springframework.org/schema/aop/spring-aop.xsd
			http://www.springframework.org/schema/jee
			http://www.springframework.org/schema/jee/spring-jee.xsd">

	<!-- オブジェクト検索時の検索条件プラグインのリスト定義 -->
	<bean id="ObjectSearchConditionPlugin" class="jp.co.ctc_g.eim.framework.search.impl.ObjectSearchConditionPlugInImpl">
		<property name="plugins" >
			<list>
			</list>
		</property>
	</bean>
	<!-- リレーション検索時の検索条件プラグインのリスト定義 -->
	<bean id="RelationSearchConditionPlugin" class="jp.co.ctc_g.eim.framework.search.impl.RelationSearchConditionPlugInImpl">
		<property name="plugins" >
			<list>
			</list>
		</property>
	</bean>
	
	<!-- 検索条件プラグインの設定対象と検索条件生成クラスのリスト -->
	<!-- ドキュメント検索用 -->
	<bean id="SampleObjectPlugin" class="jp.co.ctc_g.eim.framework.search.impl.SearchConditionBase">
		<property name="plugins">
			<list>
			</list>
		</property>
		<property name="type">
			<value>ドキュメント</value>
 		</property>
	</bean>
	<!-- リレーション検索用 -->
	<bean id="SampleRelationPlugin" class="jp.co.ctc_g.eim.framework.search.impl.SearchConditionBase">
		<property name="plugins">
			<list>
			</list>
		</property>
		<property name="target">
			<value>RELATION</value>
		</property>
		<property name="type">
			<value>ドキュメント</value>
		</property>
	</bean>

	<!-- 検索条件生成プラグイン実体クラス定義 -->
	<!-- 
	<bean id="SampleCondition" class="jp.co.ctc_g.eim.framework.search.impl.SampleConditionCreater" />
	 -->
	 
	 
	
</beans>


