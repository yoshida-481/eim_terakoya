<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:mvc="http://www.springframework.org/schema/mvc"
	xsi:schemaLocation="
			http://www.springframework.org/schema/beans
			http://www.springframework.org/schema/beans/spring-beans.xsd
        	http://www.springframework.org/schema/mvc
        	http://www.springframework.org/schema/mvc/spring-mvc.xsd">

	<!-- ======================================================================================== -->
	<!-- 文書管理用Bean定義                                                                       -->
	<!-- ======================================================================================== -->

	<bean id="DocumentFileTransferController" class="jp.co.ctc_g.eim.app.document.presentation.controller.DocumentFileTransferController"  parent="FileTransferController">
		<property name="objectService" ref="objectService2"/>
		<property name="accessHistoryService" ref="accessHistoryService2"/>
		<property name="formAttachedFileService" ref="formAttachedFileService"/>
	</bean>

	<bean id="DocumentFormTypeController" class="jp.co.ctc_g.eim.app.document.presentation.controller.DocumentFormTypeController">
		<property name="documentFormTypeService" ref="documentFormTypeServiceForHandlingValueList"/>
	</bean>

	<bean id="DocumentFormController" class="jp.co.ctc_g.eim.app.document.presentation.controller.DocumentFormController">
		<property name="documentFormService" ref="htmlDocumentFormService"/>
	</bean>

	<bean id="ThumbnailPreviewController" class="jp.co.ctc_g.eim.app.document.presentation.controller.ThumbnailPreviewController">
		<property name="thumbnailPreviewService" ref="thumbnailPreviewService"/>
	</bean>

	<bean id="CoverDocumentFormController" class="jp.co.ctc_g.eim.app.document.presentation.controller.CoverDocumentFormController">
		<property name="coverDocumentFormService" ref="coverDocumentFormService"/>
	</bean>

	<bean id="DocumentFileObjectCreatorController" class="jp.co.ctc_g.eim.app.document.presentation.controller.DocumentFileObjectCreatorController">
		<property name="fileObjectCreatorService" ref="documentFileObjectCreatorService"/>
	</bean>

	<bean id="WebDAVController" class="jp.co.ctc_g.eim.app.document.presentation.controller.WebDAVController">
		<property name="webDAVService" ref="documentWebDAVService"/>
	</bean>

	<bean id="PublicDocumentController" class="jp.co.ctc_g.eim.app.document.presentation.controller.PublicDocumentController">
		<property name="publicDocumentService" ref="publicDocumentService"/>
		<property name="objectTypeService" ref="objectTypeService2"/>
		<property name="objectService" ref="objectServiceWithoutAttribute"/>
	</bean>
	<!-- ワークフロー管理コントローラ(ドキュメント管理用) -->
	<bean id="WorkFlowProcController" class="jp.co.ctc_g.eim.common.presentation.web.controller.WorkFlowProcController">
	</bean>

	<bean id="FoloderUploadController" class="jp.co.ctc_g.eim.common.presentation.web.controller.LumpFolderController">
		<property name="folderUploadService" ref="folderUploadService"/>
	</bean>
</beans>