package jp.co.ctc_g.eim.admin.presentation.web.form.criteria;

import java.util.List;

/**
 * キャッシュエントリーの検索条件DTO
 * 
 */
public class CacheEntrySearchCriteriaForm {

	/** ノードID */
	private long nodeId;

	/** キャッシュ領域キー */
	private String cacheSpaceKey;

	/** 最大取得件数 */
	private long maxCount;

	/** IDリスト */
	private List<String> idList;


	/**
	 * ノードIDを取得します。<br>
	 * @return ノードID
	 */
	public long getNodeId() {
		return nodeId;
	}

	/**
	 * ノードIDを設定します。
	 * @param cacheSpaceKey ノードID
	 */
	public void setNodeId(long nodeId) {
		this.nodeId = nodeId;
	}

	/**
	 * キャッシュ領域キーを取得します。<br>
	 * @return キャッシュ領域キー
	 */
	public String getCacheSpaceKey() {
		return cacheSpaceKey;
	}

	/**
	 * キャッシュ領域キーを設定します。
	 * @param cacheSpaceKey キャッシュ領域キー
	 */
	public void setCacheSpaceKey(String cacheSpaceKey) {
		this.cacheSpaceKey = cacheSpaceKey;
	}

	/**
	 * 最大取得件数を取得します。<br>
	 * @return 最大取得件数
	 */
	public long getMaxCount() {
		return maxCount;
	}

	/**
	 * 最大取得件数を設定します。
	 * @param maxCount 最大取得件数
	 */
	public void setMaxCount(long maxCount) {
		this.maxCount = maxCount;
	}

	/**
	 * 検索対象IDリストを取得します。<br>
	 * @return 検索対象IDリスト
	 */
	public List<String> getIdList() {
		return idList;
	}

	/**
	 * 検索対象IDリストを設定します。
	 * @param idList 検索対象IDリスト
	 */
	public void setIdList(List<String> idList) {
		this.idList = idList;
	}

}
